    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.7",
    "@angular/cli": "^20.3.7",
    "@angular/compiler-cli": "^20.3.0",
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "jest-preset-angular": "^15.0.3",
